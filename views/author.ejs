    <title>Portfolio</title>
  </head>

  <body>
    <!-- FRAME -->
    <div class="frame">
      <!-- MAIN-FRAME -->
      <div class="main_frame">
        <!-- LEFT-FRAME -->
        <div class="left_frame">
          <div class="left1">
            <!-- PROFILE -->
            <div class="left_1-1">
              <img class="" src="images/profile.png" width="156" height="142" />
            </div>
            <div class="left_1-2"><%=user.name%></div>
            <div class="left_1-3"><%=user.profession%></div>
            <div class="left-icons">
              <a class="link_href" href="">
                <div class="icon">
                  <img src="icon/Facebook.svg" class="icon-1" />
                </div>
              </a>
              <a class="link_href" href="">
                <div class="icon">
                  <img src="icon/Instagram.svg" class="icon-1" />
                </div>
              </a>
              <a class="link_href" href="">
                <div class="icon">
                  <img src="icon/Linkeding.svg" class="icon-1" />
                </div>
              </a>
              <a class="link_href" href="">
                <div class="icon">
                  <img src="icon/Twitter.svg" class="icon-1" />
                </div>
              </a>
              <a class="link_href" href="">
                <div class="icon">
                  <img src="icon/Youtube.svg" class="icon-1" />
                </div>
              </a>
              <a class="link_href" href="">
                <div class="icon">
                  <img src="icon/Dribbble.svg" class="icon-1" />
                </div>
              </a>
            </div>
          </div>
          <div class="marginer"></div>
          <!-- PERSONAL-INFO -->
          <div class="left2">
            <div class="info__wrapper">
              <div class="info__container">
                <p class="info__title">Age:</p>
                <p class="info__info"><%=user.age%></p>
              </div>
              <div class="info__container">
                <p class="info__title">Residence:</p>
                <p class="info__info"><%=user.residence%></p>
              </div>
              <div class="info__container">
                <p class="info__title">Freelance:</p>
                <p class="info__info info_green">Available</p>
              </div>
              <div class="info__container">
                <p class="info__title">Address:</p>
                <p class="info__info"><%=user.address%></p>
              </div>
            </div>
          </div>
          <div class="marginer"></div>
          <!-- LANGUAGES -->
          <div class="left3">
            <h3 class="left3__title">Languages</h3>
            <div class="languages__info">
              <p class="language__name"><%=user.languages[0]%></p>
              <p class="language_percent">100%</p>
            </div>
            <div class="percent-line">
              <div class="line-inside percent-line__bangla"></div>
            </div>
            <div class="languages__info">
              <p class="language__name"><%=user.languages[2]%></p>
              <p class="language_percent">80%</p>
            </div>
            <div class="percent-line">
              <div class="line-inside percent-line__english"></div>
            </div>
              <div class="languages__info">
                <p class="language__name"><%=user.languages[1]%></p>
                <p class="language_percent">60%</p>
              </div>
              <div class="percent-line">
                <div class="line-inside percent-line__spanish"></div>
              </div>
            </div>
            <div class="marginer"></div>
            <!-- SKILLS -->
            <div class="left4">
              <p class="skills__title">Skills</p>
              <div class="skills__container">
                <p class="skills__name"><%=user.skills[0]%></p>
                <p class="skills__percent">90%</p>
              </div>
              <div class="percent-line">
                <div class="line-inside percent-line__html"></div>
              </div>
              <div class="skills__container">
                <p class="skills__name"><%=user.skills[1]%></p>
                <p class="skills__percent">85%</p>
              </div>
              <div class="percent-line">
                <div class="line-inside percent-line__css"></div>
              </div>
              <div class="skills__container">
                <p class="skills__name"><%=user.skills[2]%></p>
                <p class="skills__percent">80%</p>
              </div>
              <div class="percent-line">
                <div class="line-inside percent-line__js"></div>
              </div>
              <div class="skills__container">
                <p class="skills__name"><%=user.skills[3]%></p>
                <p class="skills__percent">75%</p>
              </div>
              <div class="percent-line">
                <div class="line-inside percent-line__php"></div>
              </div>
              <div class="skills__container">
                <p class="skills__name"><%=user.skills[4]%></p>
                <p class="skills__percent">85%</p>
              </div>
               <div class="percent-line">
                <div class="line-inside percent-line__wordpress"></div>
              </div>
            </div>
            <div class="marginer"></div>
            <!-- EXTRA-SKILLS -->
            <div class="left5">
              <p class="extra-skills__title">Extra Skills</p>
              <ul class="extra-skills__list">
                <li class="extra-skills__item">
                  <img
                    class="extra-skills__img"
                    src="icon/extra-skills.svg"
                    alt=""
                  />
                  <p class="extra-skills__text">Bootstrap, Materialize</p>
                </li>
                <li class="extra-skills__item">
                  <img
                    class="extra-skills__img"
                    src="icon/extra-skills.svg"
                    alt=""
                  />
                  <p class="extra-skills__text">Stylus, Sass, Less</p>
                </li>
                <li class="extra-skills__item">
                  <img
                    class="extra-skills__img"
                    src="icon/extra-skills.svg"
                    alt=""
                  />
                  <p class="extra-skills__text">Gulp, Webpack, Grunt</p>
                </li>
                <li class="extra-skills__item">
                  <img
                    class="extra-skills__img"
                    src="icon/extra-skills.svg"
                    alt=""
                  />
                  <p class="extra-skills__text">GIT Knowledge</p>
                </li>
              </ul>
            </div>
            <div class="marginer"></div>
          </div>

